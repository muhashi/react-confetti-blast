{
  "name": "react-confetti-blast",
  "version": "1.0.2",
  "author": {
    "name": "muhashi",
    "email": "hello@muhashi.com",
    "url": "https://muhashi.com"
  },
  "description": "A lightweight css-animation based confetti exploder for React",
