    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.35.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
